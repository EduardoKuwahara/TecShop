{
  "expo": {
    "name": "TecShop",
    "slug": "TecShop",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "tecshop",
    "splash": {
      "backgroundColor": "#FFA800",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.eduardokjr.tecshop",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#FFA800"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "3c4c625d-f378-408d-a5dd-b0cb51e602b0"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
